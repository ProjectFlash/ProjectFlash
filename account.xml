<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/Account"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:background="@android:color/holo_green_dark"
        android:text="Make Set "
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="98dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/imageView5"
        app:srcCompat="@drawable/ic_home_black_24dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="94dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:srcCompat="?android:attr/actionModeFindDrawable" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="105dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_menu_myplaces" />


    <EditText
        android:id="@+id/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="6"
        android:hint="Sgoodman12"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="@color/black"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Account" />

    <EditText
        android:id="@+id/SetOne"
        android:layout_width="404dp"
        android:layout_height="173dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/darker_gray"
        android:ems="8"
        android:hint="CS101"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button" />

    <EditText
        android:id="@+id/SetTwo"
        android:layout_width="402dp"
        android:layout_height="173dp"
        android:layout_marginTop="10dp"
        android:background="@android:color/darker_gray"
        android:ems="8"
        android:hint="History"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="@+id/SetOne"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/SetOne"
        app:layout_constraintTop_toBottomOf="@+id/SetOne" />

    <Button
        android:id="@+id/Button"
        android:layout_width="147dp"
        android:layout_height="83dp"
        android:backgroundTint="@android:color/holo_red_dark"
        android:text="Drag Down"
        app:layout_constraintBottom_toBottomOf="@+id/Bio"
        app:layout_constraintStart_toEndOf="@id/Bio"
        app:layout_constraintTop_toTopOf="@+id/Bio" />

    <TextView
        android:id="@+id/Bio"
        android:layout_width="253dp"
        android:layout_height="138dp"
        android:text="Total sets: 5 Avg rating: 200 My Sets"
        android:textColor="@color/black"
        android:textSize="35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="95dp"
        android:layout_height="78dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:srcCompat="@android:drawable/ic_input_add" />

    <TextView
        android:id="@+id/SetOneCreatedBy"
        android:layout_width="244dp"
        android:layout_height="70dp"
        android:text="Created By You"
        android:textColor="@color/black"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/SetOne" />

    <TextView
        android:id="@+id/SetTwoCreatedBy"
        android:layout_width="286dp"
        android:layout_height="74dp"
        android:text="Created By Brenda"
        android:textColor="@color/black"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/SetTwo"
        />
</androidx.constraintlayout.widget.ConstraintLayout>