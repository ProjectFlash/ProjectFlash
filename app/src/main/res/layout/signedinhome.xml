<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/home"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:background="@color/teal_700"
        android:text="Home "
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/firstset"
        android:layout_width="411dp"
        android:layout_height="157dp"
        android:layout_margin="0dp"
        android:text=" Biology Set"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home" />

    <RatingBar
        android:id="@+id/ratingone"
        android:layout_width="238dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/firstset"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/pfpone"
        android:layout_width="34dp"
        android:layout_height="44dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingone"
        app:layout_constraintStart_toStartOf="@+id/firstset"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/usernameone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingone"
        app:layout_constraintStart_toEndOf="@+id/pfpone" />

    <TextView
        android:id="@+id/secondset"
        android:layout_width="411dp"
        android:layout_height="157dp"
        android:layout_margin="0dp"
        android:text=" Math prblms"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstset" />

    <ImageButton
        android:id="@+id/pfptwo"
        android:layout_width="34dp"
        android:layout_height="44dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingtwo"
        app:layout_constraintStart_toStartOf="@+id/secondset"
        tools:srcCompat="@tools:sample/avatars" />
    <RatingBar
        android:id="@+id/ratingtwo"
        android:layout_width="238dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/secondset"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/usernametwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingtwo"
        app:layout_constraintStart_toEndOf="@+id/pfptwo" />
    <TextView
        android:id="@+id/thirdset"
        android:layout_width="411dp"
        android:layout_height="157dp"
        android:layout_margin="0dp"
        android:text=" PE Terms"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondset" />

    <RatingBar
        android:id="@+id/ratingthree"
        android:layout_width="238dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/thirdset"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/usernamethree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingthree"
        app:layout_constraintStart_toEndOf="@+id/pfpthree" />

    <TextView
        android:id="@+id/fourthset"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:text=" History set"
        android:textColor="@color/black"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thirdset" />

    <ImageView
        android:id="@+id/pfpthree"
        android:layout_width="34dp"
        android:layout_height="44dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingthree"
        app:layout_constraintStart_toStartOf="@+id/thirdset"
        tools:srcCompat="@tools:sample/avatars" />
    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="98dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/imageView5"
        app:srcCompat="@drawable/ic_home_black_24dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="94dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:srcCompat="?android:attr/actionModeFindDrawable" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="105dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_menu_myplaces" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="95dp"
        android:layout_height="78dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:srcCompat="@android:drawable/ic_input_add" />
</androidx.constraintlayout.widget.ConstraintLayout>